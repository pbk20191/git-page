		"gh-pages": "^6.3.0",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5"
	},
	"dependencies": {
		"comlink": "^4.4.2",
		"elheif": "file:elheif-0.1.2.tgz",
