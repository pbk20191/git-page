{
	"name": "git-page",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"homepage": "https://pbk20191.github.io/git-page",
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"dev": "vite dev --host",
		"build": "vite build",
